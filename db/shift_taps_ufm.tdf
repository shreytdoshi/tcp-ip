FUNCTION altsyncram_c681 (address_a[3..0], address_b[3..0], clock0, clocken0, data_a[7..0], wren_a)
RETURNS ( q_b[7..0]);
FUNCTION cntr_qif (clk_en, clock)
RETURNS ( q[3..0]);

--synthesis_resources = lut 4 M9K 1 reg 4 
SUBDESIGN shift_taps_ufm
( 
	clken	:	input;
	clock	:	input;
	shiftin[7..0]	:	input;
	shiftout[7..0]	:	output;
	taps[7..0]	:	output;
) 
VARIABLE 
	altsyncram2 : altsyncram_c681;
	cntr1 : cntr_qif;

BEGIN 
	altsyncram2.address_a[] = cntr1.q[];
	altsyncram2.address_b[] = cntr1.q[];
	altsyncram2.clock0 = clock;
	altsyncram2.clocken0 = clken;
	altsyncram2.data_a[] = ( shiftin[]);
	altsyncram2.wren_a = B"1";
	cntr1.clk_en = clken;
	cntr1.clock = clock;
	shiftout[7..0] = altsyncram2.q_b[7..0];
	taps[] = altsyncram2.q_b[];
END;
